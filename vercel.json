{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "framework": "express",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/dist/index.js"
    },
    {
      "source": "/:path*",
      "destination": "/dist/index.js"
    }
  ],
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  }
}
